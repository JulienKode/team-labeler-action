{
  "name": "team-labeler-action",
  "version": "2.0.0",
  "private": true,
  "description": "Action to add a label based on the team",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build",
    "test": "jest",
    "build:pack": "yarn build && yarn package",
    "all": "yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JulienKode/team-labeler-action.git"
  },
  "keywords": [
    "actions",
    "github",
    "team",
    "label",
    "labeller",
    "node"
  ],
  "author": "Julien Karst",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.11.1",
    "@actions/github": "6.0.0",
    "js-yaml": "4.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.25.1",
    "@types/jest": "29.5.14",
    "@types/js-yaml": "4.0.9",
    "@types/node": "20.17.30",
    "@typescript-eslint/eslint-plugin": "8.29.1",
    "@typescript-eslint/parser": "8.31.1",
    "@vercel/ncc": "0.38.3",
    "eslint": "9.0.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-prettier": "5.1.3",
    "globals": "16.0.0",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "js-yaml": "4.1.0",
    "prettier": "3.5.3",
    "ts-jest": "29.3.2",
    "typescript": "5.8.3"
  }
}
