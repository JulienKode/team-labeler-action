{
  "name": "team-labeler-action",
  "version": "2.0.2",
  "private": true,
  "description": "Action to add a label based on the team",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build",
    "test": "jest",
    "build:pack": "yarn build && yarn package",
    "all": "yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JulienKode/team-labeler-action.git"
  },
  "keywords": [
    "actions",
    "github",
    "team",
    "label",
    "labeller",
    "node"
  ],
  "author": "Julien Karst",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.11.1",
    "@actions/github": "6.0.1",
    "js-yaml": "4.1.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@types/jest": "30.0.0",
    "@types/js-yaml": "4.0.9",
    "@types/node": "25.0.3",
    "@typescript-eslint/eslint-plugin": "8.49.0",
    "@typescript-eslint/parser": "8.48.1",
    "@vercel/ncc": "0.38.4",
    "eslint": "9.39.1",
    "eslint-plugin-jest": "29.2.1",
    "eslint-plugin-prettier": "5.5.4",
    "globals": "16.5.0",
    "jest": "30.2.0",
    "jest-circus": "30.2.0",
    "js-yaml": "4.1.1",
    "prettier": "3.7.3",
    "ts-jest": "29.4.5",
    "typescript": "5.9.3"
  }
}
